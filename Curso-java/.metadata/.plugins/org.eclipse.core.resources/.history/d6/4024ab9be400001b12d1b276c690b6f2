package classe.desafio;

public class Jantar {
	
	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa("Rafael", 80);
		Comida a = new Comida("Feijão", 0.225);
		double result = p1.comer(a);
		
		System.out.println("O(a)" + p1.nome + " comeu " + a.nome + " e passou de " + p1.peso + " kg para " + result + "kg");
	}

}
