package fundamentos.operadores;

public class Atribuicao {
	
	public static void main(String[] args) {
		
		int a = 3; // Valores
		int b = a; // Outras variáveis
		int c = a + b; // Uma expressão
		
		c += b; // = c = c + b;
		c -= a;
		c *= b;
		c /= a;
		System.out.println(c);
		c %= 2; // c = c % 2
		
		
		
	}

}
