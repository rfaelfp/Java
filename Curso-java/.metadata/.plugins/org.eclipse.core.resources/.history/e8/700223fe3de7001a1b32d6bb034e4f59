package fundamentos;

public class NotacaoPonto {
	
	public static void main(String[] args) {
		
		double a = 2.3;
		String s = "Bom dia X"; // A declaração de string no java é com letra maiuscula. Não é tipo primitivo e não é palavra reservada.
		s = s.toUpperCase();
		
		System.out.println(s);
		
	}

}
